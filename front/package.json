{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "test": "jest --verbose --testRegex='^((?!(integration|manual)).)*.test.ts$'",
    "test:manual": "jest --runInBand --watchAll --testRegex=.manual.test.ts",
    "setup-env-vars": "chmod +x ./env.sh && ./env.sh",
    "setup-env-vars-build": "chmod +x ./env.sh && ./env.sh ./dist/env-config.js",
    "dev-in-memory": "VITE_GATEWAY=IN_MEMORY pnpm setup-env-vars && vite",
    "dev": "export $(cat .env | grep 'VITE_' | xargs) && pnpm setup-env-vars && vite",
    "make-styles": "scss-to-ts update",
    "build-scalingo": "rm -rf scalingo/dist && vite build && mv dist scalingo/dist && cp env.sh scalingo/dist && tar -czf front-build.tar.gz scalingo",
    "build": "vite build && export $(cat .env | grep 'VITE_' | xargs) && pnpm setup-env-vars-build",
    "serve": "vite preview --port 5001",
    "typecheck": "tsc --noEmit",
    "includecheck": "./check_includes.sh",
    "check": "pnpm -w check:glob ./front/src",
    "prettier": "pnpm -w prettier:glob --write ./front/src",
    "format:check": "pnpm -w prettier:glob --check ./front/src",
    "lint": "pnpm -w lintconfig:glob ./front/src",
    "fullcheck": "pnpm -w fullcheck --filter=front",
    "postinstall": "copy-dsfr-to-public",
    "predev": "only-include-used-icons",
    "prebuild": "only-include-used-icons"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@codegouvfr/react-dsfr": "^1.9.8",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^2.9.11",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@reduxjs/toolkit": "^1.9.7",
    "@sentry/browser": "^7.110.0",
    "@sentry/vite-plugin": "^2.16.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.31",
    "@types/papaparse": "^5.3.14",
    "@types/ramda": "^0.28.25",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.25",
    "@types/react-lines-ellipsis": "^0.15.6",
    "@types/react-redux": "^7.1.33",
    "@types/swagger-ui-react": "^4.18.3",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "babel-jest": "^29.7.0",
    "date-fns": "2.28.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jest": "^28.2.0",
    "html-templates": "workspace:*",
    "jest": "^29.7.0",
    "openapi-types": "^12.1.3",
    "papaparse": "^5.4.1",
    "postcss": "^8.4.38",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-design-system": "workspace:*",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.51.3",
    "react-redux": "^7.2.9",
    "redux": "^4.2.1",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.8.1",
    "sass": "^1.75.0",
    "scss-mapper": "workspace:*",
    "shared": "workspace:*",
    "shared-routes": "^0.8.17",
    "swagger-ui-react": "^5.15.1",
    "ts-pattern": "^5.1.1",
    "tss-react": "^4.9.6",
    "type-route": "^1.1.0",
    "typescript": "^5.4.5",
    "uuid": "^8.3.2",
    "vite": "^4.5.3",
    "vite-plugin-pwa": "^0.17.5",
    "workbox-window": "^7.0.0",
    "zod": "3.22.3"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1"
  }
}
